[user]
    email = kdbomhof@gmail.com
    name = senyc
[core]
    editor = nvim
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = always
[alias]
    br = branch
    ca = commit --amend
    cae = commit --amend --no-edit
    co = checkout
    rb = rebase
    s = status
    sw = switch
    a = add
    ap = add -p
    aa = add --all 
    c = commit
    cm = commit -m
    d = diff
    ds = diff --staged
    p = push
    ri = rebase -i
    graph = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)'
[apply]
    whitespace = nowarn
[url "git@github.com:"]
    insteadOf = "gh:"
[pull]
    rebase = true
