#!/usr/bin/env bash

config_path="$PWD"

valid_basename() {
    [ "$1" != "." ] && [ "$1" != ".." ] 
}

  for directory in "$config_path"/.* "$config_path"/*; do
      dir_base_name=$(basename "$directory")
      if valid_basename "$dir_base_name"; then
          echo "$dir_base_name"
      fi
  done
